{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "new-cap": 1,
      "camelcase": "off",
      "global-require": "off",
      "max-len": ["error", { "code": 120 }],
      "no-return-await": "off"
    }
}
